code = "// NOTE: Shader automatically converted from Godot Engine 4.6.stable's StandardMaterial3D.

shader_type spatial;
render_mode blend_mix, depth_draw_opaque, cull_back, diffuse_burley, specular_schlick_ggx, unshaded;

uniform vec4 albedo : source_color;
uniform sampler2D texture_albedo : source_color, filter_nearest, repeat_enable;
uniform ivec2 albedo_texture_size;
uniform float point_size : hint_range(0.1, 128.0, 0.1);

uniform float roughness : hint_range(0.0, 1.0);
uniform sampler2D texture_metallic : hint_default_white, filter_nearest, repeat_enable;
uniform vec4 metallic_texture_channel;
uniform sampler2D texture_roughness : hint_roughness_r, filter_nearest, repeat_enable;

uniform float specular : hint_range(0.0, 1.0, 0.01);
uniform float metallic : hint_range(0.0, 1.0, 0.01);

uniform vec3 uv1_scale;
uniform vec3 uv1_offset;
uniform vec3 uv2_scale;
uniform vec3 uv2_offset;

varying vec2 base_uv;

// Uniforms can be adjusted in the Inspector
uniform float radius : hint_range(0.0, 1.0) = 0.75;         // The inner radius where the fade begins (0.0 to 1.0, relative to UV)
uniform float fade_length : hint_range(0.0, 1.0) = 0.1;    // The length of the gradient fade

void vertex() {
	base_uv = UV;
	UV = UV * uv1_scale.xy + uv1_offset.xy;
}

void fragment() {
	vec2 new_uv = UV;
	vec4 albedo_tex = texture(texture_albedo, new_uv);
	ALBEDO = albedo.rgb * albedo_tex.rgb;

	float metallic_tex = dot(texture(texture_metallic, new_uv), metallic_texture_channel);
	METALLIC = metallic_tex * metallic;
	SPECULAR = specular;

	vec4 roughness_texture_channel = vec4(1.0, 0.0, 0.0, 0.0);
	float roughness_tex = dot(texture(texture_roughness, new_uv), roughness_texture_channel);
	ROUGHNESS = roughness_tex * roughness;
	ALPHA *= albedo.a * albedo_tex.a;
	
    // Calculate the distance from the center of the UV coordinates (0.5, 0.5)
    float dist = distance(base_uv, vec2(0.5));

    // Normalize distance to be 0 at center and 0.5 at the edge of a square UV
    // Multiply by 2.0 to have the edge of a proper circle at dist = 1.0 (relative to radius)
    dist *= 2.0;

    // Calculate the alpha value based on the distance
    // This value is 1.0 inside the radius and fades out over the fade_length
    float alpha = smoothstep(radius + fade_length, radius, dist);

    ALPHA *= alpha;
}
"

